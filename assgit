local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Raw4exe/Void/refs/heads/main/assgit"))()
local main = Library.new()

local tab1 = main:create_tab("Combat", "rbxassetid://76499042599127")
local tab2 = main:create_tab("Utility", "rbxassetid://126017907477623")

local mod1 = tab1:create_module({
  title = "Auto Farm",
  flag = "auto_farm_mod",
  description = "Automatic farming",
  section = "left",
  callback = function(value) print("Auto Farm:", value) end
})

mod1:create_checkbox({
  title = "Enable Farm",
  flag = "enable_farm_cb",
  callback = function(value) print("Farm enabled:", value) end
})

mod1:create_slider({
  title = "Farm Speed",
  flag = "farm_speed_sl",
  minimum = 1,
  maximum = 100,
  default = 50,
  round_number = true,
  callback = function(value) print("Speed:", value) end
})

mod1:create_dropdown({
  title = "Farm Mode",
  flag = "farm_mode_dd",
  options = {"Normal", "Fast", "Safe"},
  multi_dropdown = false,
  maximum_options = 1,
  callback = function(value) print("Mode:", value) end
})

mod1:create_button({
  title = "Start Farm",
  callback = function() print("Farm started!") end
})

mod1:create_textbox({
  title = "Farm Target",
  flag = "farm_target_tb",
  placeholder = "Enter target name",
  callback = function(value) print("Target:", value) end
})

mod1:create_divider({
  title = "Advanced",
  showtopic = true
})

mod1:create_divider({
  title = "Advanced",
  showtopic = true
})

